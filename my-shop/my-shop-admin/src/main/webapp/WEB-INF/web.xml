<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <!--default 激活Tomcat的defaultServlet来处理静态文件，不被我们定义的servlet拦截执行-->
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>/static/img/*</url-pattern>
    <url-pattern>/static/css/*</url-pattern>
    <url-pattern>/static/js/*</url-pattern>
    <url-pattern>/static/plugins/*</url-pattern>
    <url-pattern>/static/video/*</url-pattern>
  </servlet-mapping>
</web-app>
